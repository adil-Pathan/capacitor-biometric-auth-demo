{
  "name": "@aparajita/capacitor-biometric-auth-demo",
  "version": "2.0.0",
  "description": "An Ionic/Vue demo of the @aparajita/capacitor-biometric-auth plugin",
  "private": true,
  "author": "Aparajita Fishman",
  "homepage": "https://github.com/aparajita/capacitor-biometric-auth-demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/aparajita/capacitor-biometric-auth-demo.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm check.fix && pnpm build.only",
    "build.only": "vite build",
    "preview": "vite preview --port 1327",
    "lint": "eslint --cache . && pnpm typecheck",
    "lint.fix": "eslint --cache --fix . && pnpm typecheck",
    "typecheck": "vue-tsc --noEmit",
    "prettier": "prettier --check .",
    "prettier.fix": "prettier --check --write .",
    "check": "pnpm lint && pnpm prettier && echo '\u001b[32m✔︎\u001b[0mAll good!'",
    "check.fix": "pnpm lint.fix && pnpm prettier.fix && echo '\u001b[32m✔︎\u001b[0mAll good!'",
    "ios.dev": "ionic cap run ios --open --livereload --external",
    "ios.prod": "ionic cap run ios --open",
    "android.dev": "ionic cap run android --open --livereload --external",
    "android.prod": "ionic cap run android --open",
    "ionic:build": "vite build",
    "ionic:serve": "vite"
  },
  "dependencies": {
    "@aparajita/capacitor-biometric-auth": "^2.0.5",
    "@capacitor/android": "^3.6.0",
    "@capacitor/app": "^1.1.1",
    "@capacitor/core": "3.6.0",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/ios": "^3.6.0",
    "@capacitor/keyboard": "^1.2.3",
    "@capacitor/splash-screen": "^1.2.2",
    "@capacitor/status-bar": "^1.0.8",
    "@ionic/core": "^6.1.12",
    "@ionic/vue": "6.1.12",
    "@ionic/vue-router": "6.1.12",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@aparajita/eslint-config-vue": "^1.0.2",
    "@aparajita/prettier-config": "^1.0.0",
    "@capacitor/cli": "3.6.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.1.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.7.4",
    "vite": "^2.9.13",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "~0.38.2"
  }
}
